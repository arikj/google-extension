{
  "name": "Tab Switchy",
  "version": "1.0.1.0",
  "description": "Gives the freedom to play with tabs",
  "icons": {
    "16": "utility.png",
    "48": "utility48.png",
    "128": "utility128.png"
  },
 
  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "utility.png",
    "default_title": "Tab Switchy",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "<all_urls>",
    "bookmarks"
  ],
  "manifest_version": 2
}